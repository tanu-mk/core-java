		public void travel(Passport passport) {
			
			boolean isAllowed = false;
			
			System.out.println("Inside travel method ");
			
			//if(isVisaAvailable == true) {
				
				if(laguage >= 32) {
					System.out.println(" Laguage check is done ");
					
